{
  "name": "safe-client-gateway",
  "description": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn generate-abis && nest build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "generate-abis": "bun ./scripts/generate-abis.js",
    "postinstall": "bun run generate-abis",
    "start": "bun run build && bun --env-file=.env dist/src/main.js",
    "start:dev": "nest start --watch -e \"bun\"",
    "start:debug": "nest start --debug --watch -e \"bun\"",
    "start:prod": "NODE_ENV=production bun --env-file=.env dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test,migrations}/**/*.ts\" --fix",
    "lint-check": "eslint \"{src,apps,libs,test,migrations}/**/*.ts\"",
    "test": "NODE_ENV=test node ./node_modules/.bin/jest",
    "test:watch": "NODE_ENV=test node ./node_modules/.bin/jest --watch",
    "test:cov": "NODE_ENV=test node ./node_modules/.bin/jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk --require tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "test:unit": "NODE_ENV=test node ./node_modules/.bin/jest --config ./test/jest-unit.json",
    "test:unit:cov": "NODE_ENV=test node ./node_modules/.bin/jest --coverage --config ./test/jest-unit.json",
    "test:integration": "NODE_ENV=test node ./node_modules/.bin/jest --config ./test/jest-integration.json",
    "test:integration:cov": "NODE_ENV=test node ./node_modules/.bin/jest --coverage --config ./test/jest-integration.json",
    "test:e2e": "NODE_ENV=test node ./node_modules/.bin/jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "NODE_ENV=test node ./node_modules/.bin/jest --coverage --config ./test/jest-e2e.json",
    "test:all": "NODE_ENV=test node ./node_modules/.bin/jest --config ./test/jest-all.json",
    "test:all:cov": "NODE_ENV=test node ./node_modules/.bin/jest --coverage --config ./test/jest-all.json",
    "typeorm": "bun --env-file=.env -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./dist/src/config/entities/orm.config.js",
    "migration:create": "bun --env-file=.env -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./migrations/${0}",
    "migration:generate": "bun typeorm migration:generate ./migrations/${0}",
    "migration:run": "bun typeorm migration:run",
    "migration:revert": "bun typeorm migration:revert",
    "env:validate": "bun scripts/validate-env-json.ts",
    "env:validate:silent": "bun scripts/validate-env-json.ts --silent",
    "env:generate": "bun scripts/generate-env.ts",
    "env:generate:force": "bun scripts/generate-env.ts --force",
    "env:generate:update": "bun scripts/generate-env.ts --update"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "3.995.0",
    "@aws-sdk/client-s3": "3.995.0",
    "@aws-sdk/lib-storage": "3.995.0",
    "@aws-sdk/s3-request-presigner": "3.995.0",
    "@blockaid/client": "^0.65.0",
    "@fingerprintjs/fingerprintjs-pro-server-api": "6.10.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.14",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.14",
    "@nestjs/platform-express": "11.1.14",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/serve-static": "5.0.4",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "11.0.0",
    "@safe-global/safe-deployments": "1.37.51",
    "amqp-connection-manager": "5.0.0",
    "amqplib": "0.10.8",
    "bullmq": "^5.56.0",
    "cache-manager": "6.4.1",
    "cookie-parser": "1.4.7",
    "csv-stringify": "^6.6.0",
    "jsonwebtoken": "9.0.3",
    "lodash": "4.17.23",
    "nestjs-cls": "6.0.1",
    "pg": "8.18.0",
    "postgres": "3.4.7",
    "redis": "5.10.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "sanitize-html": "2.17.0",
    "semver": "7.7.1",
    "typeorm": "0.3.28",
    "viem": "2.23.5",
    "winston": "3.17.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "10.3.0",
    "@nestjs/cli": "11.0.14",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.1.14",
    "@smithy/util-stream": "4.5.5",
    "@types/amqplib": "0",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.6",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9",
    "@types/lodash": "4.17.24",
    "@types/node": "24.10.0",
    "@types/sanitize-html": "2",
    "@types/semver": "7.7.0",
    "@types/supertest": "6.0.3",
    "aws-sdk-client-mock": "4.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.7.4",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.4",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.31.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "\\.integration\\.spec\\.ts$",
      "\\.e2e-spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@faker-js).+"
    ],
    "roots": [
      "./src",
      "./migrations",
      "./scripts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "index.ts",
      ".+\\/__tests__\\/.+\\.builder.(t|j)s",
      ".+\\/__tests__\\/.+\\.factory.(t|j)s",
      ".e2e-spec.ts",
      ".integration.spec.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/abis/(.*)$": "<rootDir>/abis/$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "globalSetup": "<rootDir>/test/global-setup.ts"
  },
  "main": "main.ts",
  "repository": "https://github.com/5afe/safe-client-gateway-nest.git",
  "packageManager": "bun@1.3.9"
}
