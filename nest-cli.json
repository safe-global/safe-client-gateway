{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "gateway": {
      "type": "application",
      "root": "src",
      "entryFile": "main",
      "sourceRoot": "src"
    },
    "auth-service": {
      "type": "application",
      "root": "src",
      "entryFile": "auth-service/main",
      "sourceRoot": "src"
    }
  }
}
